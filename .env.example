# Environment variables example
# Copy this file to .env and fill with your values

# Server configuration
SERVER_HOST=your-server-ip-or-domain
SERVER_USER=ubuntu
SERVER_PORT=22

# API Configuration (JSON array format for GitHub Secrets)
API_KEYS=["your-secret-key-1", "your-secret-key-2"]

# SimpleCorsProxy Configuration
SIMPLE_CORS_REQUIRE_AUTH=true

# Deployment
DOCKER_IMAGE_NAME=webcorsproxy
CONTAINER_NAME=proxyserver
CONTAINER_PORT=8000

# Generate secure API keys with:
# openssl rand -hex 32

